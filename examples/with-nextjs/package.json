    "@refinedev/simple-rest": "^5.0.1",
    "cross-env": "^7.0.3",
    "next": "^13.5.4",
    "nookies": "^2.5.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "antd": "^5.0.5"
  },
  "devDependencies": {
    "@types/node": "^18.16.2",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
