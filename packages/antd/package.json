{
    "name": "@pankod/refine-antd",
    "version": "3.3.2",
    "description": "refine is a React-based framework for building internal tools, rapidly. It ships with Ant Design System, an enterprise-level UI toolkit.",
    "private": false,
    "main": "dist/index.js",
    "module": "dist/refine-antd.esm.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "start": "tsdx watch --tsconfig tsconfig.json --verbose --noClean",
        "build": "tsdx build --tsconfig tsconfig.json",
        "prepare": "tsdx build",
        "test": "tsdx test --passWithNoTests --runInBand",
        "size": "size-limit",
        "analyze": "size-limit --why",
        "generate-theme": "npx @emeks/antd-custom-theme-generator -w --antd ../../node_modules/antd ./src/assets/styles/custom-theme.less ./src/assets/styles/styles.min.css"
    },
    "peerDependencies": {
        "antd": "^4.18.8",
        "dayjs": "^1.10.7",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.13.14",
        "@rollup/plugin-image": "^2.0.6",
        "@size-limit/preset-small-lib": "^5.0.5",
        "@testing-library/jest-dom": "^5.12.0",
        "@testing-library/react": "^11.2.6",
        "@testing-library/react-hooks": "^5.0.3",
        "@testing-library/user-event": "^12.8.3",
        "@types/jest": "^26.0.24",
        "@types/react": "^17.0.4",
        "@types/react-dom": "^17.0.4",
        "@types/react-router-dom": "^5.1.8",
        "@types/testing-library__jest-dom": "^5.9.5",
        "babel-plugin-module-resolver": "^4.1.0",
        "identity-obj-proxy": "^3.0.0",
        "postcss": "^8.1.4",
        "react-router-dom": "^5.3.0",
        "rollup-plugin-copy": "^3.4.0",
        "rollup-plugin-postcss": "^4.0.0",
        "size-limit": "^5.0.5",
        "tsdx": "^0.14.1",
        "typescript": "^4.4.3"
    },
    "dependencies": {
        "@ant-design/icons": "^4.5.0",
        "@pankod/refine-core": "^3.3.1",
        "antd": "^4.17.1",
        "dayjs": "^1.10.7",
        "react-markdown": "^6.0.1",
        "react-query": "^3.19.2",
        "remark-gfm": "^1.0.0",
        "sunflower-antd": "1.0.0-beta.3",
        "tslib": "^2.3.1"
    },
    "author": "Pankod",
    "license": "MIT",
    "gitHead": "829f5a516f98c06f666d6be3e6e6099c75c07719",
    "publishConfig": {
        "access": "public"
    }
}
